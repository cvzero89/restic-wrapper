{
	"FTP": [
		{
			"enabled": true,
			"type": "sftp",
			"host": "user@host",
			"repo_path": "path_to_repo",
			"backup_path": "path_to_backup",
			"password_file": "password_file_location",
			"options": {
				"no-scan": true,
				"compression": "auto", 
				"read-concurrency": false
			},
			"forget_options": {
				"daily": 1,
				"weekly": 2,
				"monthly": 2
			},
			"exclude": "this,testing"
		}
		],
	"S3": [
		{
			"enabled": true,
			"type": "s3",
			"host": "",
			"repo_path": "https://examples3url.com/repo_name/",
			"backup_path": "path_to_backup",
			"password_file": "password_file_location",
			"options": {
				"no-scan": true,
				"compression": "auto", 
				"read-concurrency": false,
				".env-file": ".s3.env"
			},
			"forget_options": {
				"daily": 1,
				"weekly": 2,
				"monthly": 2
			},
			"exclude": "anothertest,other"
		}	
	],
	"local": [
		{
			"enabled": false,
			"type": "local",
			"host": "",
			"repo_path": "path_to_repo",
			"backup_path": "path_to_backup",
			"password_file": "password_file_location",
			"options": {
				"no-scan": false,
				"compression": "auto", 
				"read-concurrency": false
			},
			"forget_options": {
				"daily": 1,
				"weekly": 2,
				"monthly": 2
			},
			"exclude": ""
		}
	]
}
